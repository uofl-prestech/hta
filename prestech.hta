<!DOCTYPE html>
<html>

<head>
    <title>OOP Task Sequence Selections</title>
    <meta http-equiv="x-ua-compatible" content="ie=9">
    <link rel="stylesheet" href="./jquery-ui-1.11.4.custom/jquery-ui.css">
    <script src="./jquery-1.11.1.min.js"></script>
    <script src="./jquery-ui-1.11.4.custom/jquery-ui.min.js"></script>
    <script src="./HTAfunctions.vbs" type="text/vbscript" language="vbscript"></script>
    <script src="./bitlockerFunctions.vbs" type="text/vbscript" language="vbscript"></script>
    <script type="text/vbscript" language="vbscript">
        window.resizeTo 1024,768
        window.moveTo 400, 200
    </script>
    <link rel="stylesheet" href="Prestech.css">
    <hta:application applicationname="OOP Task Sequence Selections" border="thin" innerborder="no" caption="yes" scroll="no" windowstate="normal"></hta:application>
</head>

<body>
    <div id="container">
        <div id="header">
            <h1>// PRESTECH HTA</h1>
        </div>
        <div id="content">
            <div class="navigation-bar">
                <ul>
                    <li id="button-osd" class="first">
                        <img src="images/osd final.png" class="icon">
                        <span>OSD</span>
                    </li>
                    <li id="button-flushfill">
                        <img src="images/flush and fill final.png" class="icon">
                        <span>Flush and Fill</span>
                    </li>
                    <li id="button-dism">
                        <img src="images/DISM final.png" class="icon">
                        <span>DISM</span>
                    </li>
                    <li id="button-usmt">
                        <img src="images/usmt final.png" class="icon">
                        <span>USMT</span>
                    </li>
                    <li id="button-software">
                        <img src="images/software install.png" class="icon">
                        <span>Software Install</span>
                    </li>
                    <li id="button-tools">
                        <img src="images/tools-icon.png" class="icon">
                        <span>Tools</span>
                    </li>
                </ul>
                <button class="button" id="button-home">Home</button>
                <button class="button" id="button-exit" onclick="vbscript:ButtonExitClick()">Exit</button>
            </div>

            <!--Landing Page-->
            <div id="page-landing" class="pages">
                <div class="category-header">OOP Task Sequence Selections</div>
            </div>

            <!--OSD Page-->
            <div id="page-osd" class="pages">
                <div class="category-header">Operating System Deployment</div>
                <div id="osd-input" class="input-div">
                    <ul>
                        <li class="list-osd">
                            <div id="stepOSD"><input type="checkbox" name="OSD"> Format Drive and Deploy Image </div><br></li>
                        <li class="list-osd">
                            <div id="stepCompName"> Enter Computer Name: <br><br> <input type="text" name="compName"> </div><br></li>
                        <li class="list-osd">
                            <div id="stepOSDSoftware"> Select Software to Install: <br>
                                <h4><input type="checkbox" class="software-osd" name="OSDAdobeReaderDC"> Adobe Reader DC
                                </h4>
                                <h4><input type="checkbox" class="software-osd" name="OSDAdobeAcrobatProXI"> Adobe Acrobat Pro XI</h4>
                                <h4><input type="checkbox" class="software-osd" name="OSDChrome"> Chrome 58</h4>
                                <h4><input type="checkbox" class="software-osd" name="OSDFilemMaker"> FileMaker Pro 16</h4>
                                <h4><input type="checkbox" class="software-osd" name="OSDOffice2016"> Office 2016 (32 Bit)</h4>
                            </div>
                        </li>
                    <button class="button" id="button-finish" onclick="vbscript:ButtonFinishClick()">Launch OSD</button>
                    </ul>
                </div>
                    <!-- <div id="osdOutput" class="output-div"></div> -->
            </div>

            <!--Flush and Fill Page-->
            <div id="page-flushfill" class="pages">
                <div class="category-header">OS Upgrade with User Data Migration: "Flush and Fill"</div>
            </div>

            <!--DISM Page-->
            <div id="page-dism" class="pages">
                <div class="category-header">Deployment Image Servicing and Management</div>
                <div id="dism-input" class="input-div">
                    <ul>
                        <li class="dism-steps">
                            <div id="dism-step1">
                                Run Bitlocker Info Tool <br><br>
                                <button class="button" id="bl-info" name="Bitlocker"><img src="images/bitlocker-icon.png" class="icon-tools">Bitlocker Info</button><br><br>
                            </div>
                        </li>
                        <li class="dism-steps">
                            <div id="dism-step2">
                                Enter the drive letter of the Windows partition to be imaged: <br><br> Drive Letter:
                                <input name="windowsDrive" type="Text" maxlength="1" size="15">
                            </div>
                        </li>
                        <li class="dism-steps">
                            <div id="dism-step3">
                                Enter the Recovery Key to unlock Bitlocker: <br><br> <input name="blKey" type="Text" maxlength="100" size="35" value="083028-485397-003157-146949-446820-226391-406692-473858"><br><br>
                                <button class="button" id="bl-unlock" name="blUnlock"><img src="images/bitlocker-unlock-icon.png" class="icon-tools">Bitlocker Unlock</button>
                            </div>
                        </li>
                        <li class="dism-steps">
                            <div id="dism-step4">
                                Enter the username of the user we are copying data for: <br><br> <input name="dismUsername" type="Text" size="15">
                            </div>
                        </li>
                        <li class="dism-steps">
                            <div id="dism-step5">
                                 Enter the drive letter of the External USB drive where the image will be stored: <br><br>
                                 Drive Letter: <input name="dismDrive" type="Text" maxlength="1" size="15">
                            </div>
                        </li>
                        <li class="dism-steps">
                            <div id="dism-step6">
                                Capture Image <br><br>
                                <input type="button" id="dismCap" value="Capture Image" name="capture"><br><br>
                            </div>
                        </li>
                    </ul>
                </div>
                <!-- <div id="dism-output" class="output-div"></div> -->
            </div>

            <!--USMT Page-->
            <div id="page-usmt" class="pages">
                <div class="category-header">User State Migration Tool</div>
            </div>

            <!--Software Install Page-->
            <div id="page-software-install" class="pages">
                <div class="category-header">Software Installation</div>
                <div id="software-input" class="input-div">
                    <input type="checkbox" name="AdobeReaderDC"> Adobe Reader DC<br>
                    <input type="checkbox" name="AdobeAcrobatProXI"> Adobe Acrobat Pro XI<br>
                    <input type="checkbox" name="Chrome"> Chrome 58<br>
                    <input type="checkbox" name="FileMaker"> FileMaker Pro 16<br>
                    <input type="checkbox" name="Office2016"> Office 2016 (32 Bit)<br>
                    <input type="checkbox" name="MBAM"> Bitlocker Agent<br>
                </div>
            </div>

            <!--Troubleshooting Tools Page-->
            <div id="page-tools" class="pages">
                <div class="category-header">Troubleshooting Tools</div>
                <div class="input-div">
                    <ul>
                        <li>
                        <button class="button" id="button-bitlocker" onclick="vbscript:ButtonFinishClick()"><img src="images/bitlocker-icon.png" class="icon-tools">Bitlocker Info</button>
                        </li>
                        <li>
                            <button class="button" id="button-cmdprompt" name="cmdWindow" onclick="vbscript:cmdPrompt()"><img src="images/command-prompt-icon.png" class="icon-tools">Command Prompt</button>
                        </li>
                        <li>
                            <button class="button" id="button-explorer" name="explorer" onclick="vbscript:explorer()"><img src="images/explorer-icon.png" class="icon-tools">Explorer ++</button>
                        </li>
                        <li>
                            <button class="button" id="button-logviewer" name="logViewer" onclick="vbscript:logViewer()"><img src="images/cmtrace-icon.png" class="icon-tools">Log Viewer</button>
                        </li>
                        <li>
                            <button class="button" id="button-pingtest" name="ping" onclick="vbscript:pingTest()"><img src="images/ping-icon.png" class="icon-tools">Ping Test</button>
                        </li>                      
                    </ul>
                </div>
                <div id="tools-output" class="output-div"></div>
            </div>
            <div id="general-output" class="output-div">placeholder</div>
        <!--    End content    -->
        </div>

        <!-- <div id="footer"> -->
            <!-- <button class="button" id="button-home">Home</button> -->
            <!-- <button class="button" id="button-exit" onclick="vbscript:ButtonExitClick()">Exit</button> -->
        <!-- </div> -->

    <!--    End container    -->
    </div>
</body>

<script language="JavaScript">
    $(document).ready(function () {
        $('.pages').css('display', 'none');
        $('#page-landing').css('display', 'block');
    });

    $('#button-osd').on('click', function() {
        $('.pages').css('display', 'none');
        $('#page-osd').css('display', 'block');
        $('#page-osd').append('<div id="general-output" class="output-div">placeholder</div>');
        // TPMCheck();
    });
    $('#button-flushfill').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-flushfill').css('display', 'block');
    });
    $('#button-dism').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-dism').css('display', 'block');
    });
    $('#button-usmt').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-usmt').css('display', 'block');
    });
    $('#button-software').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-software-install').css('display', 'block');
    });
    $('#button-tools').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-tools').css('display', 'block');
    });     
    $('#button-home').on('click', function () {
        $('.pages').css('display', 'none');
        $('#page-landing').css('display', 'block');
    });



    // $(function () {
    //     $("#dismOutput").dialog({ 
    //         autoOpen: false,
    //         width: 700,
    //         height: 500,
    //         modal: true });
    // });
    $('#blInfo').on('click', function () {
        // $("#dismOutput").dialog("option", "title", "Bitlocker Info");
        // $("#dismOutput").dialog("open");
        BitlockerInfo();
    });
    $('#blUnlock').on('click', function () {
        // $("#dismOutput").dialog("option", "title", "Bitlocker Unlock Info");
        // $("#dismOutput").dialog("open");
        BitlockerUnlock();
    });
     $('#dismCap').on('click', function () {
        // $("#dismOutput").dialog("option", "title", "DISM Capture Info");
        // $("#dismOutput").dialog("open");
        dismCapture();
    });

    // $('div').hover(
    //   function() {
    // 	$( this ).css('background', 'rgba(255,64,64,0.4)');
    //   }, function() {
    // 	$( this ).css('background', 'none');
    //   }
    // );

</script>

</html>