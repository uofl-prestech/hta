<!DOCTYPE html>
<html>

<head>
    <title>OOP Task Sequence Selections</title>
    <meta http-equiv="x-ua-compatible" content="ie=9">
    <link rel="stylesheet" href="./js/jquery-ui-1.11.4.custom/jquery-ui.css">
    <link rel="stylesheet" href="./js/jquery.mCustomScrollbar.css">
    <script src="./js/html5shiv.min.js"></script>
    <script src="./js/html5shiv-printshiv.min.js"></script>
    <script src="./js/jquery-1.11.1.min.js"></script>
    <script src="./js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="./js/jquery-ui-1.11.4.custom/jquery-ui.min.js"></script>
    <script src="./js/spin.js"></script>
    <script src="./js/jsFunctions.js" defer></script>
    <script type="text/javascript">
        function jsObj() {
            this.setProp = function (drive, k, v) {
                if (!(this.hasOwnProperty(drive))) {
                    this[drive] = {};
                }
                this[drive][k] = v;
            }
        }

        function CreateJsObj() {
            return new jsObj();
        }
    </script>
    <script src="./HTAfunctions.vbs" type="text/vbscript" language="vbscript"></script>
    <script src="./bitlockerFunctions.vbs" type="text/vbscript" language="vbscript"></script>
    <script type="text/vbscript" language="vbscript">
        window.resizeTo 1024,768
    </script>
    <link rel="stylesheet" href="Prestech.css">
    <hta:application applicationname="OOP Task Sequence Selections" border="thin" innerborder="no" caption="no" scroll="no" windowstate="normal"></hta:application>
</head>

<body>
    <div id="hidden-inputs" class="accordion">
        <input id="windows-drive-letter" class="input-objects" type="hidden" name="windowsDriveLetter">
        <input id="input-tpm-checkbox" class="input-objects" type="hidden" name="TPMCheckBox">
    </div>

    <div id="minervaDiv">
        <img src="images/minervas/minerva_white.png" id="minerva" class="icon">
    </div>

    <button id="button-landing" class="button-header button-exithome">
        <div class="button-background">
            <img src="images/home-45.svg" class="icon-only" title="Home page">
        </div>
    </button>

    <button id="button-exit" class="button-header button-exithome" onclick="vbscript:ButtonExitClick()" title="Exit program">
        <div id="div-exit" class="button-background">
            <img src="images/exit-just-circle-45.svg" id="portal-exit-circle" class="icon-only">
            <div id="portal">
                <img src="images/exit-no-circle-45.svg" id="portal-exit" class="icon-only">
            </div>
        </div>
    </button>


    <div id="container">
        <!-- ******************** Page Header Section ******************** -->
        <div id="header">
                <!-- <button id="button-landing" class="button-header button-exithome">
                    <div class="button-background">
                        <img src="images/home-45.svg" class="icon-only" title="Home page">
                    </div>
                </button> -->
                <button class="button-header" id="button-ShowAll">
                    <div class="button-background">
                        <img src="images/expand-45.svg" class="icon-only">
                    </div>
                </button>
                <button class="button-header" id="button-HideAll">
                    <div class="button-background">
                        <img src="images/minimize-45.svg" class="icon-only">
                    </div>
                </button>
            <div class="category-header">
                <h4 id="header-landing" class="header-text active-header">OOP Task Sequence Selections</h4>
                <h4 id="header-osd" class="header-text">Operating System Deployment</h4>
                <h4 id="header-flushfill" class="header-text">OS Upgrade with User Data Migration: "Flush and Fill"</h4>
                <h4 id="header-dism" class="header-text">Deployment Image Servicing and Management</h4>
                <h4 id="header-usmt" class="header-text">
                    <button class="small-button usmtStates" id="button-ScanState">Scan</button>
                    <button class="small-button usmtStates" id="button-LoadState">Load</button> User State Migration Tool
                </h4>
                <h4 id="header-software" class="header-text">Software Installation</h4>
                <h4 id="header-tools" class="header-text">Troubleshooting Tools</h4>
            </div>
                <!-- <button id="button-exit" class="button-header button-exithome" onclick="vbscript:ButtonExitClick()" title="Exit program">
                    <div id="div-exit" class="button-background">
                        <img src="images/exit-just-circle-45.svg" id="portal-exit-circle" class="icon-only">
                        <div id="portal">
                            <img src="images/exit-no-circle-45.svg" id="portal-exit" class="icon-only">
                        </div>
                    </div>
                </button> -->
        </div>
        <div id="main" class="on-load">

            <!-- ******************** Navigation Button Section ******************** -->
            <div class="navigation-bar">
                <ul>
                    <li id="button-osd" class="button-nav">
                        <img src="images/osd-monitor-75.svg" class="icon">
                        <span>OSD</span>
                    </li>
                    <li id="button-flushfill" class="button-nav require-win-dir">
                        <img src="images/flush-and-fill-monitor-75.svg" class="icon">
                        <span>Flush and Fill</span>
                    </li>
                    <li id="button-dism" class="button-nav require-win-dir">
                        <img src="images/dism-monitor-75.svg" class="icon">
                        <span>DISM</span>
                    </li>
                    <li id="button-usmt" class="button-nav require-win-dir">
                        <img src="images/usmt-monitor-75.svg" class="icon">
                        <span>USMT</span>
                    </li>
                    <!-- <li id="button-software" class="button-nav require-win-dir clearfix">
                        <img src="images/software-install.png" class="icon">
                        <span>Software Install</span>
                    </li> -->
                    <li id="button-binary" class="button-nav clearfix">
                        <img src="images/binary-capture-monitor-75.svg" class="icon">
                        <span>Binary Capture</span>
                    </li>
                </ul>
            </div>

            <!-- ******************** Page Content Section ******************** -->
            <div id="content">
                <!--Landing Page-->
                <div id="page-landing" class="pages active-page">
                    <ul id="ul-landing" class="accordion ui-accordion ul-elements"></ul>
                </div>

                <!--OSD Page-->
                <div id="page-osd" class="pages">
                    <ul id="ul-osd" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button" id="launch-osd" onclick="javascript:launchOSD()">
                            <img src="images/osd-launch-45.svg" class="icon-tools">Launch OSD</button>
                    </div>
                </div>

                <!-- Flush and Fill Page -->
                <div id="page-flushfill" class="pages">
                    <ul id="ul-flushfill" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button show-output" id="fnf-capture" onclick="javascript:launchFNF()">
                            <img src="images/flush-and-fill-launch-45.svg" class="icon-tools">Launch FnF</button>
                    </div>
                </div>

                <!--DISM Page-->
                <div id="page-dism" class="pages">
                    <ul id="ul-dism" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button show-output" id="dism-capture" onclick="javascript:launchDISM()">
                            <img src="images/dism-launch-45.svg" class="icon-tools">Launch DISM</button>
                    </div>
                </div>

                <!--USMT Page-->
                <div id="page-usmt" class="pages">
                    <ul id="ul-usmt" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button show-output" id="run-usmt" onclick="javascript:launchScanstate()">
                            <img src="images/usmt-launch-45.svg" class="icon-tools">Launch Scanstate</button>
                        <button class="button show-output" id="run-LoadState" onclick="javascript:launchLoadstate()">
                        <img src="images/load-state-45.svg" class="icon-tools">Launch LoadState</button>
                    </div>
                </div>

                <!--Software Install Page-->
                <!-- <div id="page-software" class="pages">
                    <ul id="ul-software" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button show-output" id="run-software-install" onclick="javascript:launchSoftwareInstall()">
                            <img src="images/software-install.png" class="icon-tools">Install Software</button>
                    </div>
                </div> -->
                <div id="page-binary" class="pages">
                    <ul id="ul-binary" class="accordion ui-accordion ul-elements"></ul>
                    <div class="launch-box">
                        <button class="button show-output" id="run-binary-capture" onclick="javascript:launchBinaryCapture()">
                            <img src="images/binary-capture-45.svg" class="icon-tools">Launch Binary Capture</button>
                    </div>
                </div>

                <!-- ******************** Output Section ******************** -->
                <div id="general-output-scroll">
                    <ul id="general-output" class="ui-accordion">
                        <li id="output-bl-info" class="output-list">
                            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                                <img src="images/list-view-icon-45.svg" class="icon-tools output-icons">
                                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Drive List</h3>
                            <div id="bl-info-output" class="ui-widget-content"></div>
                        </li>
                        <li id="output-bl-unlock" class="output-list">
                            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                                <img src="images/unlock-45.svg" class="icon-tools output-icons">
                                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Bitlocker Unlock</h3>
                            <div id="bl-unlock-output" class="ui-widget-content"></div>
                        </li>
                        <li id="li-output-network" class="output-list">
                            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                                <img src="images/ping-45.svg" class="icon-tools output-icons">
                                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Network Output</h3>
                            <div id="div-output-network" class="ui-widget-content"></div>
                        </li>
                        <li id="output-dism" class="output-list">
                            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                                <img src="images/dism-launch-45.svg" class="icon-tools output-icons">
                                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>DISM Output</h3>
                            <div id="dism-output" class="ui-widget-content"></div>
                        </li>
                        <li id="output-scanstate" class="output-list">
                            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                                <img src="images/usmt-launch-45.svg" class="icon-tools output-icons">
                                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Scanstate Output</h3>
                            <div id="scanstate-output" class="ui-widget-content"></div>
                        </li>
                    </ul>
                    <!-- <div id="general-output"></div> -->
                </div>
                <!--    End content    -->
            </div>
            <!--    End main    -->
        </div>

        <!-- ******************** Footer Button Bar Section ******************** -->
        <div id="footer" class="on-load">
            <div class="tools-button-bar">
                <button class="button-toolbar button-bl-info show-output" id="button-bitlocker" onclick="javascript:blInfoClick()">
                    <div class="button-background button-screen">
                        <img src="images/lock-45.svg" class="icon-only" title="Bitlocker Info">
                    </div>
                </button>
                <button class="button-toolbar" id="button-cmdprompt" name="cmdWindow" onclick="vbscript:cmdPrompt()">
                    <div class="button-background button-screen">
                        <img src="images/command-prompt-45.svg" class="icon-only" title="Command Prompt">
                    </div>
                </button>
                <button class="button-toolbar" id="button-explorer" name="explorer" onclick="vbscript:explorer()">
                    <div class="button-background button-screen">
                        <img src="images/Explorerpp-45.svg" class="icon-only" title="Explorer ++">
                    </div>
                </button>
                <button class="button-toolbar" id="button-logviewer" name="logViewer" onclick="vbscript:logViewer()">
                    <div class="button-background button-screen">
                        <img src="images/log-viewer-45.svg" class="icon-only" title="Log Viewer">
                    </div>
                </button>
                <button class="button-toolbar show-output" id="button-pingtest" name="ping" onclick="vbscript:pingTest()">
                    <div class="button-background button-screen">
                        <img src="images/ping-45.svg" class="icon-only" title="Ping Test">
                    </div>
                </button>
                <button class="button-toolbar" id="button-psPrompt" name="ps" onclick="vbscript:psPrompt()">
                    <div class="button-background button-screen">
                        <img src="images/powershell-45.svg" class="icon-only" title="Powershell">
                    </div>
                </button>
                <button class="button-toolbar" id="button-npp" name="notepadpp" onclick="vbscript:notepadPP()">
                    <div class="button-background button-screen">
                        <img src="images/notepadpp-45.svg" class="icon-only" title="Notepad ++">
                    </div>
                </button>
                <button class="button-toolbar" id="button-ftk" name="ftk" onclick="vbscript:FTK()">
                    <div class="button-background button-screen">
                        <img src="images/binary-capture-45.svg" class="icon-only" title="Forensic ToolKit">
                    </div>
                </button>
                <button class="button-toolbar" id="button-WMIExplorer" name="WMIExplorer" onclick="vbscript:WMIExplorer()">
                    <div class="button-background button-screen">
                        <img src="images/WMI-viewer-45.svg" class="icon-only" title="WMI Explorer">
                    </div>
                </button>
            </div>
        </div>
        <!--    End container    -->
    </div>

    <!-- Dialog Box -->
    <div id="dialog" title="Warning"></div>

    <!-- ******************** Template Section ******************** -->
    <!--    Landing Page    -->
    <template id="temp-landing-page" class="landing">
        <li id="li-tpm-check" class="li-steps">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>TPM Status</h3>
            <div id="tpm-check-output" class="ui-widget-content"></div>
        </li>
    </template>

    <!--    Bitlocker Info    -->
    <template id="temp-bitlocker-info" class="flushfill dism usmt">
        <li id="li-bitlocker-info" class="li-steps scanState">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Get Bitlocker Info
            </h3>
            <div id="div-bitlocker-info" class="ui-widget-content">
                <button id="bl-info" class="button button-bl-info show-output" onclick="javascript:blInfoClick()">
                    <img src="images/lock-45.svg" class="icon-tools">Bitlocker Info</button>
            </div>
        </li>
    </template>

    <!--    Bitlocker Unlock    -->
    <template id="temp-bitlocker-unlock" class="landing flushfill dism usmt">
        <li id="li-bitlocker-unlock" class="li-steps scanState">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Enter the Drive Letter and Recovery Key to unlock Bitlocker
            </h3>
            <div id="div-bitlocker-unlock" class="ui-widget-content">
                <span>Drive Letter: &nbsp&nbsp </span>
                <input id="input-bitlocker-drive" class="input-objects input-text" name="blDrive" type="Text" maxlength="1" size="1" value="">
                <br>
                <span>Key: &nbsp&nbsp </span>
                <input id="input-bitlocker-key" class="input-objects input-text" name="blKey" type="Text" maxlength="100" size="35" value="">
                <button id="bl-unlock" class="button button-bl-unlock show-output" onclick="javascript:blUnlockClick()">
                    <img src="images/unlock-45.svg" class="icon-tools">Bitlocker Unlock</button>
            </div>
        </li>
    </template>

    <!--    OSD Options    -->
    <template id="temp-osd-options" class="osd">
        <li id="li-osd-options" class="li-steps">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>OSD Options
            </h3>
            <div id="div-format-drive" class="ui-widget-content">
                <input id="input-osd-checkbox" class="input-objects" type="checkbox" name="OSDCheckBox" onchange="javascript:osdCheckboxChange(this)"> Format Drive and Deploy Image (OSD)?
            </div>
        </li>
    </template>

    <!--    FnF Options    -->
    <template id="temp-fnf-options" class="flushfill">
        <li id="li-fnf-options" class="li-steps li-win-required">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Flush and Fill Options</h3>
            <div id="div-fnf-options" class="ui-widget-content">
                <input id="input-dism-checkbox" class="input-objects" type="checkbox" name="dismCheckBox"> Create Backup Image (DISM)?
                <br>
                <input id="input-scanstate-checkbox" class="input-objects" type="checkbox" name="scanStateCheckBox"> Save User Data (USMT Scanstate)?
                <br>
                <input id="input-fnfosd-checkbox" class="input-objects" type="checkbox" name="fnfOsdCheckBox" onchange="javascript:osdCheckboxChange(this)"> Format Drive and Deploy Image (OSD)?
                <br>
                <input id="input-loadstate-checkbox" class="input-objects" type="checkbox" name="loadStateCheckBox"> Restore User Data after OSD (USMT Loadstate)?
                <br>
            </div>
        </li>
    </template>

    <!--    Computer Name    -->
    <template id="temp-computer-name" class="osd flushfill">
        <li id="li-computer-name" class="li-steps">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Enter Computer Name
            </h3>
            <div id="div-computer-name" class="ui-widget-content"> Computer Name: &nbsp; &nbsp;
                <input id="input-comp-name" class="input-objects input-text placeholder" type="text" name="compName" placeholder="Computer Name">
                <br>
            </div>
        </li>
    </template>

    <!--    Windows Drive    -->
    <template id="temp-windows-drive" class="flushfill dism usmt">
        <li id="li-windows-drive" class="li-steps scanState li-win-required">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Enter the drive letter of the Windows partition to be imaged
            </h3>
            <div id="div-windows-drive" class="ui-widget-content">
                <span>Drive Letter: &nbsp&nbsp </span>
                <input id="input-windows-drive" class="drive-input input-objects input-text" name="windowsDrive" type="Text" maxlength="1"
                    size="1">
            </div>
        </li>
    </template>

    <!--    Primary Username    -->
    <template id="temp-primary-username" class="flushfill dism usmt">
        <li id="li-primary-username" class="li-steps li-win-required">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Enter the username of the user we are copying data for
            </h3>
            <div id="div-primary-username" class="ui-widget-content">
                <span>Username: &nbsp&nbsp </span>
                <input id="input-primary-username" class="input-objects input-text" name="primaryUsername" type="Text" size="15">
            </div>
        </li>
    </template>

    <!--    Map Drive Share    -->
    <template id="temp-map-drive" class="flushfill dism usmt">
        <li id="li-map-drive" class="li-steps">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>(Optional) Map a drive letter to a network share
            </h3>
            <div id="div-map-drive" class="ui-widget-content">
                <span>Username: &nbsp&nbsp </span>
                <input id="input-share-username" class="input-objects input-text" name="shareUsername" type="Text">
                <br>
                <span>Password: &nbsp&nbsp </span>
                <input id="input-share-password" class="input-objects input-text" name="sharePassword" type="Password">
                <br>
                <span>Network Path: &nbsp&nbsp </span>
                <input id="input-share-path" class="input-objects input-text" name="sharePath" type="Text">
                <br>
                <button id="map-drive" class="button" onclick="javascript:jsMapNetworkDrive()">
                    <img src="images/map-drives-45.svg" class="icon-tools">Map Drive</button>
            </div>
        </li>
    </template>

    <!--    External Drive    -->
    <template id="temp-external-drive" class="flushfill dism usmt">
        <li id="li-external-drive" class="li-steps li-win-required">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Enter the drive letter of the External USB drive where the image will be stored
            </h3>
            <div id="div-external-drive" class="ui-widget-content">
                <span>Drive Letter: &nbsp&nbsp </span>
                <input id="input-external-drive" class="drive-input input-objects input-text" name="externalDrive" type="Text" maxlength="1"
                    size="1">
            </div>
        </li>
    </template>

    <!--    Select Users    -->
    <template id="temp-select-users" class="flushfill usmt">
        <li id="li-select-users" class="li-steps li-win-required scanState">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Select users for which data will be copied
            </h3>
            <div id="div-select-users" class="ui-widget-content usmt-users">
                <button id="enum-users" class="button" onclick="javascript:showUsersClick()">
                    <img src="images/show-users-45.svg" class="icon-tools">Show Users</button>
            </div>
        </li>
    </template>

    <!--    Software Install    -->
    <template id="temp-software-install" class="osd flushfill software binary">
        <li id="li-software-install" class="li-steps li-win-required">
            <h3 class="ui-accordion-header ui-state-default ui-corner-all ui-accordion-icons">
                <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-e"></span>Select Software to Install
            </h3>
            <div id="div-software-install" class="ui-widget-content">
                <input type="checkbox" id="AdobeReaderDC" class="software-install input-objects" name="AdobeReaderDC">Adobe Reader DC
                <br>
                <input type="checkbox" id="AdobeAcrobatProXI" class="software-install input-objects" name="AdobeAcrobatProXI">Adobe Acrobat Pro XI
                <br>
                <input type="checkbox" id="Chrome" class="software-install input-objects" name="Chrome">Chrome 58
                <br>
                <input type="checkbox" id="FileMaker" class="software-install input-objects" name="FileMaker">FileMaker Pro 16
                <br>
                <input type="checkbox" id="Office2016" class="software-install input-objects" name="Office2016">Office 2016 (32 Bit)
                <br>
                <input type="checkbox" id="MBAM" class="software-install input-objects" name="MBAM">MBAM Client (Bitlocker)
                <br>
            </div>
        </li>
    </template>

</body>

</html>